
<body>
   
    <!-- Start header -->
    <div class="all-page-title page-breadcrumb">
        <div class="container text-center">
            <div class="row">
                <div class="col-lg-12">
                    <h1>Success Message</h1>
                </div>
            </div>
        </div>
    </div>
    <!-- End header -->
    <!-- Start About -->
    <div class="about-section-box">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6 text-center">
                    <div class="inner-column">
                        <h1>Thank You  <span>@ViewBag.first @ViewBag.last</span></h1>
                        <h2>Message success</h2>
                        <p>we will send message to this email:@ViewBag.mail as soon as possible </p>
                        <p> or we will contact you to this number:@ViewBag.number</p>
                        
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <img src="~/images/stuff-img-05.jpg" alt="" class="img-fluid">
                </div>
               
            </div>
        </div>
    </div>
    <!-- End About -->
   
</body>


